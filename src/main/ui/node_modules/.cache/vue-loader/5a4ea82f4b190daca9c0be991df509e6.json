      "sourcesContent": [
        "<template>\r\n  <div class=\"form pt-6\">\r\n    <div class=\"summary text-red\" v-if=\"$v.form.$error\">\r\n      Form has errors\r\n    </div>\r\n    <form @submit.prevent=\"submit\" novalidate>\r\n      <div class=\"flex justify-center my-6\">\r\n        <div class=\"px-4\">\r\n           <name-component v-model=\"form.name\" :v=\"$v.form.name\"/>\r\n        </div>\r\n        <div class=\"px-4\">\r\n          <email-component v-model=\"form.email\" :v=\"$v.form.email\" />\r\n        </div>\r\n      </div>\r\n      <div class=\"text-center\">\r\n        <button type=\"submit\" class=\"button\">\r\n          Submit\r\n        </button>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { required, email , between} from \"vuelidate/lib/validators\";\r\nimport { isNameJoe, notGmail, isEmailAvailable } from \"@/validators\";\r\nimport NameComponent from \"./NameComponent.vue\";\r\nimport EmailComponent from \"./EmailComponent.vue\";\r\n\r\nexport default {\r\n  name: \"FormComponent\",\r\n\r\n  components: { NameComponent, EmailComponent },\r\n\r\n  data() {\r\n    return {\r\n      form: {\r\n        name: \"\",\r\n        email: \"\"\r\n      }\r\n    };\r\n  },\r\n\r\n  validations: {\r\n    form: {\r\n      name: { required, between: between(2, 32) },\r\n      email: { required, email, notGmail, isEmailAvailable }\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    submit() {\r\n      this.$v.form.$touch();\r\n      // if its still pending or an error is returned do not submit\r\n      if (this.$v.form.$pending || this.$v.form.$error) return;\r\n      // to form submit after this\r\n      alert(\"Form submitted\");\r\n    }\r\n  }\r\n};\r\n</script>\r\n"
      ]
